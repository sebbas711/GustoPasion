	<queryString>
		<![CDATA[SELECT
     usuario.`Id_Usuario` AS usuario_Id_Usuario,
     usuario.`rol` AS usuario_rol,
     usuario.`tipo_identificación` AS usuario_tipo_identificación,
     usuario.`primer_nombre` AS usuario_primer_nombre,
     usuario.`segundo_nombre` AS usuario_segundo_nombre,
     usuario.`primer_apellido` AS usuario_primer_apellido,
     usuario.`segundo_apellido` AS usuario_segundo_apellido,
     usuario.`direccion` AS usuario_direccion,
     usuario.`email` AS usuario_email,
     usuario.`telefono` AS usuario_telefono,
     usuario.`estado` AS usuario_estado,
     rol.`descripcion` AS rol_descripcion
FROM
     `rol` rol INNER JOIN `usuario` usuario ON rol.`id` = usuario.`rol`

WHERE 
usuario.`Id_Usuario` = $P{id_usuario}]]>
	</queryString>
